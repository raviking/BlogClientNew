@model IList<BlogClient.Core.Objects.Tag>

<div class="tags sidebar">
    <h3>Tags</h3>
    @if (Model.Count > 0) { 
        <ul>
            @foreach (var tag in Model) { 
                <li>@Html.TagLink(tag)</li>
            }
        </ul>
    }
    else{
        <p>No Tags found!</p>
    }
</div>
